{
  "name": "Profile",
  "base": "User",
  "idInjection": true,
  "options": {
    "emailVerificationRequired": true,
    "validateUpsert": true
  },
  "properties": {
    "imageUrl": {
      "type": "string",
      "length": 10485760
    },
    "favoriteLibraries": {
      "type": [
        "object"
      ]
    }
  },
  "validations": [],
  "relations": {
    "likes": {
      "type": "hasMany",
      "model": "Like",
      "foreignKey": ""
    },
    "groups": {
      "type": "hasAndBelongsToMany",
      "model": "Group",
      "foreignKey": "groupid"
    },
    "posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "postid"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "confirm"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
