{
  "name": "Profile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Counts": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "displayName": {
      "type": "string",
      "index": {
        "unique": true
      }
    },
    "favoriteLibrary": {
      "type": "object"
    },
    "description": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "created": {
      "type": "date"
    },
    "lastUpdated": {
      "type": "date"
    },
    "hasFilledInProfile": {
      "type": "boolean"
    },
    "birthday": {
      "type": "date"
    },
    "fullName": {
      "type": "string"
    },
    "palleid": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "likes": {
      "type": "hasMany",
      "model": "Like",
      "foreignKey": ""
    },
    "groups": {
      "type": "hasAndBelongsToMany",
      "model": "Group",
      "foreignKey": ""
    },
    "posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "postid"
    },
    "comments": {
      "type": "hasAndBelongsToMany",
      "model": "Comment",
      "foreignKey": ""
    },
    "ownedGroups": {
      "type": "hasAndBelongsToMany",
      "model": "Profile",
      "foreignKey": ""
    },
    "image": {
      "type": "hasOne",
      "model": "imageCollection",
      "foreignKey": "profileImageCollection"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "profileId",
      "options": {
        "disableInclude": true
      }
    },
    "communityRoles": {
      "type": "hasAndBelongsToMany",
      "model": "CommunityRole",
      "foreignKey": ""
    },
    "quarantines": {
      "type": "hasMany",
      "model": "Quarantine",
      "foreignKey": "quarantinedProfileId"
    },
    "reviews": {
      "type": "hasMany",
      "model": "review",
      "foreignKey": "reviewownerid"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "unilogin"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "checkIfUserExists"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "checkIfDisplayNameIsTaken"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "logout"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
