{
  "name": "Post",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Counts": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    },
    "timeCreated": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "image": {
      "type": "hasOne",
      "model": "imageCollection",
      "foreignKey": "postImageCollection"
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "postid"
    },
    "owner": {
      "type": "belongsTo",
      "model": "Profile",
      "foreignKey": "postownerid"
    },
    "group": {
      "type": "belongsTo",
      "model": "Group",
      "foreignKey": "postcontainergroupid"
    },
    "video": {
      "type": "hasOne",
      "model": "videoCollection",
      "foreignKey": "postVideoCollection"
    },
    "flags": {
      "type": "hasMany",
      "model": "Flag",
      "foreignKey": "postFlagsId"
    }
  },
  "acls": [],
  "methods": {}
}
