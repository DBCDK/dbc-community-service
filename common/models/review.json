{
  "name": "review",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "review_reviewownerid_idx": "reviewownerid",
    "review_markedAsDeleted_idx": "markedAsDeleted"
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "CountsNotDeleted": true,
    "Searchable": {
      "targetIndex": "review",
      "documentType": "review",
      "enabledProperties": [
        "pid",
        "libraryid",
        "worktype",
        "content",
        "created",
        "modified",
        "rating",
        "owner",
        "image",
        "video",
        "likes",
        "markedAsDeleted"
      ],
      "filter": {
        "include": [
          {
            "relation": "owner",
            "scope": {
              "fields": [
                "displayName",
                "id"
              ],
              "include": {
                "image": {
                  "resolutions": "image"
                }
              }
            }
          },
          {
            "image": {
              "resolutions": "image"
            }
          },
          {
            "video": {
              "resolutions": "video"
            }
          },
          "likes",
          "subjects",
          "genres"
        ],
        "where": {
          "markedAsDeleted": null
        }
      },
      "triggers": [
        {
          "modelName": "ImageCollection",
          "modelFkField": "reviewImageCollection"
        },
        {
          "modelName": "VideoCollection",
          "modelFkField": "reviewVideoCollection"
        },
        {
          "modelName": "Like",
          "modelFkField": "reviewlikeid"
        }
      ]
    }
  },
  "properties": {
    "pid": {
      "type": "string",
      "required": true
    },
    "libraryid": {
      "type": "string",
      "required": true
    },
    "worktype": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "max": 25000,
      "length": 25000
    },
    "created": {
      "type": "date",
      "required": true
    },
    "modified": {
      "type": "date",
      "required": true
    },
    "rating": {
      "type": "number",
      "required": true
    },
    "markedAsDeleted": {
      "type": "boolean",
      "required": false
    },
    "palleid": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "likes": {
      "type": "hasMany",
      "model": "Like",
      "foreignKey": "reviewlikeid"
    },
    "image": {
      "type": "hasOne",
      "model": "imageCollection",
      "foreignKey": "reviewImageCollection"
    },
    "video": {
      "type": "hasOne",
      "model": "videoCollection",
      "foreignKey": "reviewVideoCollection"
    },
    "owner": {
      "type": "belongsTo",
      "model": "Profile",
      "foreignKey": "reviewownerid"
    },
    "flags": {
      "type": "hasMany",
      "model": "Flag",
      "foreignKey": "reviewFlagsId"
    },
    "genres": {
      "type": "hasAndBelongsToMany",
      "model": "BibliographicGenre",
      "foreignKey": ""
    },
    "subjects": {
      "type": "hasAndBelongsToMany",
      "model": "BibliographicSubject",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
