{
  "name": "review",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "pid": {
      "type": "string",
      "required": true
    },
    "libraryid": {
      "type": "string",
      "required": true
    },
    "worktype": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "max": 25000,
      "length": 25000
    },
    "created": {
      "type": "date",
      "required": true
    },
    "modified": {
      "type": "date",
      "required": true
    },
    "rating": {
      "type": "number",
      "required": true
    },
    "markedAsDeleted": {
      "type": "boolean",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "likes": {
      "type": "hasMany",
      "model": "Like",
      "foreignKey": "reviewlikeid"
    },
    "image": {
      "type": "hasOne",
      "model": "imageCollection",
      "foreignKey": "reviewImageCollection"
    },
    "video": {
      "type": "hasOne",
      "model": "videoCollection",
      "foreignKey": "reviewVideoCollection"
    },
    "owner": {
      "type": "belongsTo",
      "model": "Profile",
      "foreignKey": "reviewownerid"
    },
    "flags": {
      "type": "hasMany",
      "model": "Flag",
      "foreignKey": "reviewFlagsId"
    }
  },
  "acls": [],
  "methods": {}
}
